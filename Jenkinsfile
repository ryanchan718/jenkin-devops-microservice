pipeline{
	agent any
	stages {
		stage('Build'){
			steps {
				echo "Build"
			}
		}
		stage('Test'){
			steps {
				echo "Test"
			}
		}
		stage('Integration Test'){
			steps {
				echo "Integration Test"
			}
		}
	} 
	post {
			always{
				echo "Working"
			}
			success{
				echo "Success Working"
			}
			failure{
				echo "Failed to work"
			}
		}
	}
}